<archetype-descriptor xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
                      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                      xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 https://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd" name="quickstart">


    <fileSets>

        <fileSet filtered="true" encoding="UTF-8">
            <directory>src</directory>
        </fileSet>

        <fileSet filtered="false" encoding="UTF-8">
            <directory/>
            <includes>
                <include>.gitignore</include>
            </includes>
        </fileSet>

        <fileSet filtered="true" encoding="UTF-8">
            <directory/>
            <includes>
                <include>Dockerfile</include>
            </includes>
        </fileSet>

    </fileSets>


</archetype-descriptor>